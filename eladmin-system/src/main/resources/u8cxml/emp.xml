<?xml version="1.0" encoding='UTF-8'?>
<ufinterface roottag="basdoc" billtype="bspsn" replace="Y" receiver="yy" sender="1101" isexchange="Y" filename="人员档案样本数据文件.xml" proc="add">
<basdoc id="1231560102">
        <!--以下为人员档案内容-->
		<basdoc_head>
			<!--人员基本档案主键,20位，可为空，由外系统指定或U8 cloud系统自动生成,要保证唯一-->
			<pk_psndoc></pk_psndoc>			
			<!--公司主键,不能为空,需要参照基础数据(公司主键)-->
			<pk_corp>yk</pk_corp>			
			<!--人员类别,不能为空,需要参照基础数据(人员类别)-->
			<pk_psncl>测试人员yk</pk_psncl>			
			<!--人员编码,不能为空-->
			<psncode>yk</psncode>			
			<!--姓名,不能为空-->
			<psnname>yk</psnname>				
			<!--部门主键,不能为空,需要参照基础数据(部门主键)-->
			<pk_deptdoc>部门yk</pk_deptdoc>
			<!--业务员标志,(不能为空,必须填写Y/N),默认N-->
			<clerkflag>N</clerkflag>
			<!--业务员编号,可以为空-->
			<clerkcode></clerkcode>
			<!--人员岗位,可以为空,需要参照基础数据(人员岗位)-->
			<pk_om_job></pk_om_job>
			<!--已转入档案标识,不能为空(Y/N)-->
			<indocflag>Y</indocflag>
			<!--归属范围,不能为空，应与人员类别的归属范围相同(0：在职人员；1：应聘人员；2：解聘人员；3：离退人员)-->
			<psnclscope>0</psnclscope>
			<!--助记码,可以为空-->
			<amcode></amcode>			
			<!--以下是人员辅助信息-->			
			<!--身份证号,可以为空-->
			<id>123456789</id>
			<!--社会保障号,可以为空-->
			<ssnum></ssnum>
			<!--考勤卡号,可以为空-->
			<timecardid></timecardid>
			<!--性别,(男/女)不能为空-->
			<sex>女</sex>
			<!--出生日期,可以为空-->
			<birthdate>1988-5-20</birthdate>
			<!--民族,可以为空,需要参照民族自定义档案(民族主键)-->
			<nationality></nationality>
			<!--血型,可以为空,(O,A,B,AB)-->
			<bloodtype></bloodtype>
			<!--婚姻状况,可以为空,需要参照婚姻状况自定义档案(婚姻状况主键)-->
			<marital></marital>
			<!--健康状况,可以为空,需要参照健康状况自定义档案(健康状况主键)-->
			<health></health>
