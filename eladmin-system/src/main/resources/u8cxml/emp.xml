			<indutydate>2004-12-05</indutydate>
			<!--参加工作时间,可以为空-->
			<joinworkdate>2003-05-20</joinworkdate>
			<!--离职时间,可以为空-->
			<outdutydate></outdutydate>
			<!--邮政编码,可以为空-->
			<postalcode></postalcode>
			<!--家庭住址,可以为空-->
			<addr></addr>
			<!--电子邮件,可以为空-->
			<email></email>
			<!--办公电话,可以为空-->
			<officephone></officephone>
			<!--手机,可以为空-->
			<mobile></mobile>
			<!--BP,可以为空-->
			<bp></bp>
			<!--家庭电话,可以为空-->
			<homephone></homephone>			
			<!--审批标志,可以为空-->
			<approveflag></approveflag>
			<!--数据采集人,可以为空-->
			<dataoperator></dataoperator>
